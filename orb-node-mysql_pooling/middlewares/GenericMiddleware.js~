 module.exports = {
    name: "GenericMiddleware",
    content: "null",
    errorResponse404: function (req,res,errCode,next) {
        message = "";
        switch (errCode) {
            case 404:
                message = "404 not found";
                break;
        }
        if(message == ""){
            res.send({"status": "false", "message": message});
        }
        next();
    }
} 


